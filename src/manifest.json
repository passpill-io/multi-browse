{
  "name": "Chrome tiles",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "content_scripts": [{
    "matches": ["http://*/*","https://*/*"],
    "js": ["contentScript.bundle.js"]
  }],
  "permissions": [
    "<all_urls>","webRequest","webRequestBlocking","commands","tabs","webNavigation", "history"
  ],
  "commands": {
    "split-window": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Split current window"
    }
  }
}
